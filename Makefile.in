##############################################################################
# The below rules should only be needed by developers.
##############################################################################

cpanm:
	cpanm --quiet --installdeps --notest .; \
	CPANM_RESULT=$$?; \
	if [ $$CPANM_RESULT != 0 ]; then \
	    echo ---------------------------------------------------; \
	    cat ~/.cpanm/build.log; \
